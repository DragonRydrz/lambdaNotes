{
  "expo": {
    "name": "Lambda Notes",
    "description":
      "A simple note app.  Passwords are secured, please do not save sensitive data in your notes.",
    "slug": "lambdanotes",
    "privacy": "public",
    "sdkVersion": "27.0.0",
    "platforms": ["ios", "android"],
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/appIcon.png",
    "primaryColor": "#A0001D",
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "androidStatusBar": {
      "barStyle": "dark-content",
      "backgroundColor": "#FFFFFF"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "buildNumber": "5",
      "supportsTablet": true,
      "bundleIdentifier": "com.ajgenung.lambdanotes"
    },
    "android": {
      "package": "com.ajgenung.lambdanotes",
      "versionCode": 5,
      "permissions": []
    }
  }
}
